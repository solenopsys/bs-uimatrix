{
  "$schema": "../../node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "fr-uimatrix": {
      "root": "src",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser-esbuild",
          "options": {
            "outputPath": "../../dist/fronts/fr-uimatrix",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "./src/polyfills.ts",
            "tsConfig": "tsconfig.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              {
                "input": "../../libraries/solenopsys/ui-templates/src/assets",
                "glob": "**/*",
                "output": "assets"
              },
              {
                "input": "../../libraries/solenopsys/ui-icons/src/assets",
                "glob": "**/*",
                "output": "assets"
              },
              {
                "input": "../../libraries/solenopsys/ui-charts/src/stories",
                "glob": "**/*",
                "output": "assets/stories/charts"
              },
              {
                "input": "../../libraries/solenopsys/ui-controls/src/stories",
                "glob": "**/*",
                "output": "assets/stories/controls"
              },
              {
                "input": "../../libraries/solenopsys/ui-forms/src/stories",
                "glob": "**/*",
                "output": "assets/stories/forms"
              },
              {
                "input": "../../libraries/solenopsys/ui-layouts/src/stories",
                "glob": "**/*",
                "output": "assets/stories/layouts"
              },
              {
                "input": "../../libraries/solenopsys/ui-lists/src/stories",
                "glob": "**/*",
                "output": "assets/stories/lists"
              },
              {
                "input": "../../libraries/solenopsys/ui-modals/src/stories",
                "glob": "**/*",
                "output": "assets/stories/modals"
              },
              {
                "input": "../../libraries/solenopsys/ui-navigate/src/stories",
                "glob": "**/*",
                "output": "assets/stories/navigate"
              },
              {
                "input": "../../libraries/solenopsys/ui-publications/src/stories",
                "glob": "**/*",
                "output": "assets/stories/publications"
              },
              {
                "input": "../../libraries/solenopsys/ui-templates/src/stories",
                "glob": "**/*",
                "output": "assets/stories/templates"
              }
            ],
            "styles": [
              "../../libraries/solenopsys/ui-templates/src/assets/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "fr-uimatrix:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "fr-uimatrix:build:production"
            }
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "d1d8a92e-7192-42d8-942d-7071ed505a62"
  }
}